[
    {
        "shell": [
            {
                "command": "command -v tmux >/dev/null 2>&1 || echo 'tmux not installed'; exit 0",
                "stderr": true,
                "stdin": true,
                "stdout": true
            }
        ]
    },
    {
        "clean": ["~"]
    },
    {
        "link": {
            "~/.tmux.conf": "tmux.conf"
        }
    }
]
