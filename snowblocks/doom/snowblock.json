[
    {
        "clean": ["$HOME/.doom.d"]
    },
    {
        "link": {
            "$HOME/.doom.d": {
                "path": "doomd",
                "create": true
            }
        }
    },
    {
        "shell": [
            {
                "command": "command -v emacs >/dev/null 2>&1 || echo 'emacs not installed'; exit 0",
                "stderr": true,
                "stdin": true,
                "stdout": true
            }
        ]
    }

]
